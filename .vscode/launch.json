{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug T.c",
            "type": "cppdbg", // Utilisez "cppdbg" pour le langage C
            "request": "launch",
            "program": "${workspaceFolder}/Progc/prog_t",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build T.c",
            "miDebuggerPath": "/path/to/your/gdb"
        }
    ]
}
